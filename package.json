  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/aws-lambda": "^8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
