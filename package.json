{
  "name": "@castri1/ctr-router",
  "version": "1.0.0",
  "type": "module",
  "description": "A minimalistic router for AWS Lambda functions",
  "keywords": [
    "router"
  ],
  "author": "Daniel Castrillon <castri1@hotmail.com>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "tsup",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write",
    "clean": "rimraf node_modules"
  },
  "dependencies": {
    "aws-lambda": "^1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/aws-lambda": "^8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
