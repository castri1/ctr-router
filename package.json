{
  "name": "@castri1/ctr-router",
  "version": "1.0.0",
  "type": "module",
  "description": "A minimalistic router for AWS Lambda functions",
  "keywords": [
    "router"
  ],
  "author": "Daniel Castrillon <daniel@melonn.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
